        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BattleStart"></activity>
        <activity android:name=".CharacterOrganization" />
        <activity android:name=".CharacterDetails" />
        <activity android:name=".CharacterMakeConpletion" />
