        <activity android:name=".CharacterCheck" />
        <activity android:name=".CharacterList" />
        <activity android:name=".TopScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>