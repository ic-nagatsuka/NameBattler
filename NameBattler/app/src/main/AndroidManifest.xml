<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.namebattler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.namebattler.activity.BattleResult"></activity>
        <activity android:name="com.namebattler.activity.StrategyChange" />
        <activity android:name="com.namebattler.activity.BattleMain" />
        <activity android:name="com.namebattler.activity.BattleStart" />
        <activity android:name="com.namebattler.activity.CharacterOrganization" />
        <activity android:name="com.namebattler.activity.CharacterDetails" />
        <activity android:name="com.namebattler.activity.CharacterMakeConpletion" />
        <activity android:name="com.namebattler.activity.CharacterMake" />
        <activity android:name="com.namebattler.activity.CharacterList" />
        <activity android:name="com.namebattler.activity.TopScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>